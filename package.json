{
  "name": "combinator",
  "version": "0.6.0",
  "description": "Find multiple concurrent <script> or <link> tags and turn 'em into single, combo-handled tags",
  "preferGlobal": true,
  "author": "Pat Cavit <pcavit@gmail.com",
  "bugs": "https://github.com/tivac/node-combinator/issues",
  "bin": "./bin/cli.js",
  "main": "./lib/combinator.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tivac/node-combinator.git"
  },
  "dependencies": {
    "async": "^1.5.1",
    "concat-stream": "^1.5.0",
    "escape-string-regexp": "^1.0.3",
    "foreach": "^2.0.5",
    "htmlparser2": "^3.8.3",
    "npmlog": "^2.0.0",
    "optimist": "~0.6.0",
    "shelljs": "^0.5.3",
    "traverse": "0.6.x"
  },
  "devDependencies": {
    "istanbul": "^0.4.2",
    "mocha": "^2.3.0"
  },
  "engine": {
    "node": ">=0.8"
  },
  "scripts": {
    "test": "istanbul test --print both ./node_modules/mocha/bin/_mocha"
  },
  "keywords": [
    "html",
    "parsing",
    "webperf"
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "curly": 2,
      "dot-notation": 2,
      "eol-last": 0,
      "key-spacing": [
        2,
        {
          "align": "value "
        }
      ],
      "no-caller": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-eval": 2,
      "no-ex-assign": 2,
      "no-extend-native": 2,
      "no-extra-parens": 0,
      "no-extra-semi": 2,
      "no-floating-decimal": 2,
      "no-func-assign": 2,
      "no-implied-eval": 2,
      "no-inner-declarations": 2,
      "no-mixed-requires": 0,
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "no-negated-in-lhs": 2,
      "no-new": 0,
      "no-obj-calls": 2,
      "no-sparse-arrays": 2,
      "no-trailing-spaces": 0,
      "no-underscore-dangle": 0,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 2
    }
  }
}
