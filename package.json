{
    "name"          : "combinator",
    "version"       : "0.1.0",
    "description"   : "Find multiple concurrent <script> or <link> tags and turn 'em into single, combo-handled tags",
    "preferGlobal"  : true,
    "author"        : "Pat Cavit <pcavit@gmail.com",
    "bugs"          : "https://github.com/tivac/node-combinator/issues",
    "bin"           : "./bin/cli.js",
    "main"          : "./lib/combinator.js",
    "license"       : "MIT",
    
    "repository": {
        "type" : "git",
        "url"  : "https://github.com/tivac/node-combinator.git"
    },
    
    "dependencies" : {
        "htmlparser" : "1.7.x",
        "wrench"     : "1.3.x",
        "optimist"   : "0.3.x",
        "traverse"   : "0.6.x",
        "async"      : "0.1.x",
        "mkdirp"     : "0.3.x",
        "parseUri"   : "1.2.x"
    },
    
    "devDependencies" : {
        "mocha"  : "1.7.x"
    },
    
    "engine": {
        "node": ">=0.8"
    },
    
    "scripts" : {
        "test" : "mocha"
    },
    
    "keywords": [
        "html",
        "parsing",
        "webperf"
    ]
}
