{
    "name"          : "combinator",
    "version"       : "0.1.5",
    "description"   : "Find multiple concurrent <script> or <link> tags and turn 'em into single, combo-handled tags",
    "preferGlobal"  : true,
    "author"        : "Pat Cavit <pcavit@gmail.com",
    "bugs"          : "https://github.com/tivac/node-combinator/issues",
    "bin"           : "./bin/cli.js",
    "main"          : "./lib/combinator.js",
    "license"       : "MIT",
    
    "repository": {
        "type" : "git",
        "url"  : "https://github.com/tivac/node-combinator.git"
    },
    
    "dependencies" : {
        "htmlparser2" : "3.3.x",
        "wrench"      : "1.5.x",
        "optimist"    : "0.6.x",
        "traverse"    : "0.6.x",
        "async"       : "0.2.x",
        "mkdirp"      : "0.3.x",
        "parseUri"    : "1.2.x"
    },
    
    "devDependencies" : {
        "mocha"    : "1.13.x",
        "istanbul" : "0.1.x"
    },
    
    "engine": {
        "node": ">=0.8"
    },
    
    "scripts" : {
        "test" : "istanbul test ./node_modules/mocha/bin/_mocha"
    },
    
    "keywords": [
        "html",
        "parsing",
        "webperf"
    ]
}
